{
  "name": "@cougargrades/raster",
  "version": "20.0.6",
  "description": "Simple grid system",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cougargrades/raster.git"
  },
  "scripts": {
    "watch": "nodemon --ext '*' --watch src/ --watch res/ --exec npm run build",
    "build": "postcss src/raster.css src/grid.css src/debug.css --dir dist/ && cp res/* dist/"
  },
  "keywords": [
    "html",
    "css",
    "grid",
    "web"
  ],
  "author": "Rasmus Andersson <https://rsms.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cougargrades/raster/issues"
  },
  "homepage": "https://github.com/cougargrades/raster#readme",
  "devDependencies": {
    "autoprefixer": "^9.7.6",
    "cssnano": "^4.1.10",
    "nodemon": "^2.0.3",
    "postcss-banner": "^3.0.1",
    "postcss-cli": "^7.1.0",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.1"
  }
}
